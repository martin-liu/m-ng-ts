{
  "name": "m-ng-ts",
  "version": "0.1.0",
  "description": "Angular 1.5 & Typescript",
  "main": "app.js",
  "scripts": {
    "start": "npm run dll && webpack-dev-server --content-base app/ --progress --colors --inline --hot --compress --history-api-fallback --port 8888",
    "setup": "npm install && gulp init",
    "clean-docs": "rimraf docs/**/*",
    "clean-build": "rimraf dist/**/*",
    "clean": "npm run clean-docs && npm run clean-build",
    "build-prod": "npm run clean && webpack --env=prod --progress --colors && gulp build",
    "build-dev": "npm run clean && webpack --env=dev --progress --colors && gulp build",
    "dll": "rimraf ./app/assets/dll/* && webpack --config webpack.dll.js",
    "test": "karma start --singleRun",
    "test:watch": "karma start"
  },
  "keywords": [
    "angular",
    "typescript",
    "bootstrap",
    "gulp",
    "webpack",
    "graphql",
    "rest",
    "soap",
    "socketio",
    "protobuf",
    "karma",
    "chai",
    "mocha",
    "jasmine",
    "protractor",
    "angular2.0"
  ],
  "author": "Martin Liu",
  "license": "ISC",
